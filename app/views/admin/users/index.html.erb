<div class="row">
  <div class="col-xs-12">
    <div class="row">
      <div class="col-xs-12">
        <h3 class="header smaller lighter blue"></h3>
        <div class="">
        </div>
        <div class="table-responsive">
          <table id="sample-table-2" class="table table-striped table-bordered table-hover">
            <thead>
              <tr>
                <th>登录号</th>
                <th>状态</th>
                <th>结束时间</th>
                <th>推荐人</th>
                <th>权限名字</th>
                <th>操作</th>
              </tr>
            </thead>
            <% @users.each do |user| %>
              <tbody>
                <tr>
                  <td><%= user.email %></td>
                  <td><%= user.status %></td>
                  <td><%= user.end_time %></td>
                  <td><%= @hash[user.father_id] %></td>
                  <td><%= user.role.try :title %></td>
                  <td>编辑</td>
                </tr>
              </tbody>
            <% end %>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>

